
public class VariableDeclaration {
	
	public static void main (String[] args) {
		boolean on = false;
		System.out.println(on);
		
		short s = 16;
		System.out.println(s);
		
		
		int i = -32;
		System.out.println(i);
		
		
		float f = 3833.64533f;
		System.out.println(f);
		
		long l = 123456789L;
		System.out.println(l);
	}

}
